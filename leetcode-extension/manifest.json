{
  "manifest_version": 3,
  "name": "LeetCode Extension",
  "version": "1.0.0",
  "description": "A Chrome extension for LeetCode",
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://leetcode.com/*"],
      "js": ["content.js"]
    }
  ],
  "permissions": [
    "storage",
    "activeTab"
  ],
  "host_permissions": [
    "https://leetcode.com/*",
    "http://localhost:3000/*"
  ]
}
